#!/bin/sh

bgs="49 40 41 42 43 44 45 46 47"
fgs="39 30 31 32 33 34 35 36 37"
styles="21 1"

# Column headers.
printf "      "
for bg in ${bgs}; do
    printf "  ${bg}m "
done
printf "\n"

for fg in ${fgs}; do
    for style in ${styles}; do
        # Row header.
        printf "%5sm" "${style};${fg}"

        # Print columns.
        for bg in ${bgs}; do
            printf " \e[${style};${bg};${fg}m gYw \e[0m"
        done

        printf "\n"
     done
done
